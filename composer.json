{
    "name": "accexs/laravel-ga4-event-tracking",
    "description": "Laravel package to easily send events to Google Analytics 4",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronny Arvelo"
        }
    ],
    "homepage": "https://github.com/accexs/laravel-ga4-event-tracking",
    "keywords": [
        "laravel",
        "google-analytics-4",
        "GA4",
        "laravel-analytics",
        "analytics",
        "analytics-event-tracking"
    ],
    "require": {
        "illuminate/support": "^9",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Daikazu\\GA4EventTracking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daikazu\\GA4EventTracking\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daikazu\\GA4EventTracking\\ServiceProvider"
            ]
        }
    }
}
